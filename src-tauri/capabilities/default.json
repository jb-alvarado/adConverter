{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "core:window:allow-close",
    "shell:allow-open",
    "dialog:default",
    "store:default",
    "window-state:default",
    {
      "identifier": "http:default",
      "allow": [{ "url": "https://*" }],
      "deny": []
    }
  ]
}
