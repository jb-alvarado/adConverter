{
    "name": "youtube",
    "title": "YouTube",
    "tooltip": "Encoding settings for Youtube",
    "filter_video": {
        "scale": "1920:1080"
    },
    "filter_audio": {},
    "video": {
        "-pix_fmt": "yuv420p",
        "-r": 25,
        "-c:v": "libx264",
        "-preset": "slower",
        "-crf": 21,
        "-g": 12,
        "-flags": "+cgop",
        "-bf": 2,
        "-maxrate": "15M",
        "-bufsize": "2.6M"
    },
    "audio": {
        "-c:a": "libfdk_aac",
        "-b:a": "384k",
        "-ar": "48k"
    },
    "container_video": "mp4",
    "container_audio": null
}
