{
    "name": "preview_20min",
    "title": "Preview 20min",
    "tooltip": "20min encoding for previews",
    "filter_video": {
        "scale": "640:360"
    },
    "filter_audio": {},
    "video": {
        "-t": 1200,
        "-pix_fmt": "yuv420p",
        "-aspect": "16:9",
        "-r": 25,
        "-c:v": "libx264",
        "-preset": "slower",
        "-crf": 23,
        "-g": 25,
        "-refs": 3,
        "-maxrate": "1000k",
        "-bufsize": "1000k",
        "-x264-params": "bframes=1"
    },
    "audio": {
        "-c:a": "libfdk_aac",
        "-b:a": "96k",
        "-ar": "44100"
    },
    "container_video": "mp4",
    "container_audio": null
}
